{
  "name": "just-keep-at-it",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -c green.bold,blue.bold 'pnpm dev:next' 'pnpm dev:codegen'",
    "dev:codegen": "pnpm run codegen:graphql --watch './src/**/*.ts'",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "codegen": "pnpm codegen:schema && pnpm codegen:graphql",
    "codegen:graphql": "graphql-codegen",
    "codegen:schema": "ts-node --transpile-only -r tsconfig-paths/register src/pages/api/graphql.ts",
    "clean:codegen": "trash \"./src/**/__generated__/**\"",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@formkit/auto-animate": "1.0.0-beta.3",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/near-operation-file-preset": "^2.4.2",
    "@graphql-codegen/typescript": "^2.7.4",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.9.9",
    "@pothos/core": "^3.22.6",
    "@pothos/plugin-prisma": "^3.35.4",
    "@pothos/plugin-scope-auth": "^3.15.0",
    "@pothos/plugin-simple-objects": "^3.6.4",
    "@prisma/client": "^4.4.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "graphql-helix": "^1.13.0",
    "iron-session": "^6.2.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.37.0",
    "secure-password": "^4.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.21",
    "@types/secure-password": "^3.1.1",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.18",
    "prisma": "^4.4.0",
    "tailwindcss": "^3.1.8",
    "trash-cli": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
