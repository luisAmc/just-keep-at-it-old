{
  "name": "just-keep-at-it",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -c green.bold,blue.bold 'pnpm dev:next' 'pnpm dev:codegen'",
    "dev:codegen": "pnpm run codegen:graphql --watch './src/**/*.ts'",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "codegen": "pnpm codegen:schema && pnpm codegen:graphql",
    "codegen:graphql": "graphql-codegen",
    "codegen:schema": "ts-node --transpile-only -r tsconfig-paths/register src/pages/api/graphql.ts",
    "clean:codegen": "trash \"./src/**/__generated__/**\"",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@babel/core": "^7.21.3",
    "@formkit/auto-animate": "1.0.0-beta.3",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^2.8.8",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@hookform/resolvers": "^2.9.11",
    "@pothos/core": "^3.28.0",
    "@pothos/plugin-prisma": "^3.44.0",
    "@pothos/plugin-scope-auth": "^3.18.0",
    "@pothos/plugin-simple-objects": "^3.6.7",
    "@prisma/client": "^4.11.0",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0",
    "graphql-helix": "^1.13.0",
    "iron-session": "^6.3.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.43.5",
    "secure-password": "^4.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.28",
    "@types/secure-password": "^3.1.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.21",
    "prisma": "^4.11.0",
    "tailwindcss": "^3.2.7",
    "trash-cli": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
