{
  "name": "just-keep-at-it",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -c green.bold,blue.bold 'pnpm dev:next' 'pnpm dev:codegen'",
    "dev:codegen": "pnpm codegen:graphql --watch './src/**/*.ts'",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "pnpm codegen:schema && pnpm codegen:graphql",
    "codegen:graphql": "graphql-codegen",
    "codegen:schema": "ts-node --transpile-only -r tsconfig-paths/register src/pages/api/graphql.ts",
    "clean:codegen": "trash \"./src/**/__generated__/**\"",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@apollo/client": "^3.6.7",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/near-operation-file-preset": "^2.2.14",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.9.1",
    "@pothos/core": "^3.12.0",
    "@pothos/plugin-prisma": "^3.10.0",
    "@pothos/plugin-scope-auth": "^3.9.1",
    "@pothos/plugin-simple-objects": "^3.3.0",
    "@prisma/client": "^3.15.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "graphql": "^16.5.0",
    "graphql-helix": "^1.12.0",
    "iron-session": "^6.1.3",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.32.0",
    "secure-password": "^4.0.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/secure-password": "^3.1.1",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.14",
    "prisma": "^3.15.1",
    "tailwindcss": "^3.1.2",
    "trash-cli": "^5.0.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  }
}
