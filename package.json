{
  "name": "just-keep-at-it",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -c green.bold,blue.bold,yellow.bold 'pnpm:dev:docker' 'pnpm:dev:next' 'pnpm:dev:codegen'",
    "dev:codegen": "pnpm run codegen:graphql --watch './src/**/*.ts'",
    "dev:docker": "docker compose up",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "codegen": "pnpm codegen:schema && pnpm codegen:graphql",
    "codegen:graphql": "graphql-codegen",
    "codegen:schema": "ts-node --transpile-only -r tsconfig-paths/register src/pages/api/graphql.ts",
    "clean:codegen": "trash \"./src/**/__generated__/**\"",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@babel/core": "^7.23.7",
    "@formkit/auto-animate": "1.0.0-beta.3",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@hookform/resolvers": "^3.3.4",
    "@pothos/core": "^3.41.0",
    "@pothos/plugin-prisma": "^3.63.1",
    "@pothos/plugin-scope-auth": "^3.20.0",
    "@pothos/plugin-simple-objects": "^3.7.0",
    "@prisma/client": "^5.7.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.17.6",
    "graphql": "^16.8.1",
    "graphql-helix": "^1.13.0",
    "iron-session": "^8.0.1",
    "next": "14.0.4",
    "next-pwa": "^5.6.0",
    "nprogress": "^0.2.0",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.2",
    "secure-password": "^4.0.0",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.46",
    "@types/secure-password": "^4.0.2",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.33",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.4.0",
    "trash-cli": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
